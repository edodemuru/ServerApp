<Window x:Class="ServerApp.LongTermStatistics"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"  
       
        xmlns:wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:local="clr-namespace:ServerApp"
              
        mc:Ignorable="d"
        Title="LongTermStatistics" Height="450" Width="800">
    <Grid>
        <lvc:CartesianChart Series="{Binding SeriesCollection}" LegendLocation="None" >
            <lvc:CartesianChart.AxisY>
                <lvc:Axis Title="Mac" Labels="{Binding Mac}"></lvc:Axis>
            </lvc:CartesianChart.AxisY>
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Title="Time" Labels="{Binding Time}"></lvc:Axis>
            </lvc:CartesianChart.AxisX>
            
            
        </lvc:CartesianChart>

    </Grid>
</Window>
